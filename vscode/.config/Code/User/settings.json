{
    "editor.minimap.enabled": false,
    "extensions.ignoreRecommendations": false,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "editor.smoothScrolling": true,
    "editor.autoClosingBrackets": "never",
    "editor.cursorBlinking": "solid",
    "workbench.tips.enabled": false,
    "workbench.startupEditor": "none",
    "window.restoreWindows": "all",
    "terminal.external.linuxExec": "kitty",
    "problems.decorations.enabled": true,
    "latex-workshop.view.outline.sections": [
        "chapter",
        "section",
        "subsection",
        "subsubsection",
        "paragraph"
    ],
    "latex-workshop.message.update.show": false,
    "latex-workshop.view.pdf.viewer": "browser",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "terminal.integrated.rendererType": "dom",
    "workbench.colorTheme": "Panda Syntax",
    "workbench.colorCustomizations": {
        "settings.modifiedItemForeground": "#FF7042",
        "tab.unfocusedActiveBorder": "#fff0",
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "python.pythonPath": "/usr/bin/python3",
    "workbench.activityBar.visible": true,
    "latex-workshop.latex.autoBuild.run": "never",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.useSplitJSON": true,
    "vim.foldfix": true,
    "editor.renderFinalNewline": true,
    "breadcrumbs.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.iconTheme": "eq-material-theme-icons",
    "editor.fontFamily": "'Iosevka Custom', 'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.startOnNewLine": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pylintUseMinimalCheckers": false,
    "go.formatTool": "gofmt",
    "go.useLanguageServer": true,
    "html.format.wrapLineLength": 0,
    "pretty-formatter.endComma": "always",
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.linux": "/home/simbuerg/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.helm-path.linux": "/home/simbuerg/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.draft-path.linux": "/home/simbuerg/.vs-kubernetes/tools/draft/linux-amd64/draft",
        "vscode-kubernetes.minikube-path.linux": "/home/simbuerg/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
    "vim.enableNeovim": true,
    "githubIssues.queries": [

        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Open Issues",
            "query": "state:open repo:${owner}/${repository} sort:created-desc"
        }
    ],
    "editor.fontSize": 15,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "git.ignoreLegacyWarning": true,
    "workbench.editorAssociations": {
        "*.pdf": "default",
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "go.toolsManagement.autoUpdate": true,
    "window.zoomLevel": 1,
    "redhat.telemetry.enabled": true
}